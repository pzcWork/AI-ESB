.container_in {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.container_in2 {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  height: 100%;
}
.imgTittle_out {
  width: 660rpx;
  height: 560rpx;
  margin: 0 auto;
  border-radius: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
  margin-top: 20rpx;
}
.resItem_out {
  width: 660rpx;
  height: 160rpx;
  background-color: #fff;
  border-radius: 25rpx;
  margin-top: 20rpx;
  font-size: 23rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  overflow: hidden;
}
.resItem {
  width: 660rpx;
  height: 180rpx;
  display: flex;
  justify-content: space-around;
}
.resItem_img1 {
  width: 178rpx;
  height: 178rpx;
}
.resItem > view {
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-right: 20rpx;
}
.resItem > view:first-child {
  width: 350rpx;
  margin: 0 20rpx;
}
.resItem_img {
  display: flex;
  flex-direction: column;
  align-items: center;
}
